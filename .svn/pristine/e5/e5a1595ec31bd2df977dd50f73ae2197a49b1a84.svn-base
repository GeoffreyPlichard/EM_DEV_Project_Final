<?php

namespace Mobility\PublicBundle\Entity\Coacteur;

use Doctrine\ORM\Mapping as ORM;

/**
 * Coacteur
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Mobility\PublicBundle\Entity\Coacteur\CoacteurRepository")
 */
class Coacteur
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", unique="true", nullable="false")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="email", type="string", length=255, unique="true", nullable="false")
     */
    private $email;

    /**
     * @var boolean
     *
     * @ORM\Column(name="jeu", type="boolean")
     */
    private $jeu;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set email
     *
     * @param string $email
     * @return Coacteur
     */
    public function setEmail($email)
    {
        $this->email = $email;

        return $this;
    }

    /**
     * Get email
     *
     * @return string 
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set jeu
     *
     * @param boolean $jeu
     * @return Coacteur
     */
    public function setJeu($jeu)
    {
        $this->jeu = $jeu;

        return $this;
    }

    /**
     * Get jeu
     *
     * @return boolean 
     */
    public function getJeu()
    {
        return $this->jeu;
    }
}
